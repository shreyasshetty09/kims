					$arrLast[] = $pnameArr[$i];
				}else{
					$arrPre[] = $pnameArr[$i];
				}
			}
			
			if(count($arrLast)){
				foreach($arrLast as $vs){
					$arrPre[] = $vs;
				}
			}
			
			$usedNameArr = array();
			for($i=0;$i<$numLinkKey;$i++){
				$usedNameArr[$i] = $arrPre[$i];
			}

			if(count($usedNameArr)){
				$pname = implode(' ',$usedNameArr);
